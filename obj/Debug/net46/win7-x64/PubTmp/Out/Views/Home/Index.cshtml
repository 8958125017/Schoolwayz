<style>
    div.app-spinner {
        position: fixed;
        top: 50%;
        left: 50%;
    }
</style>
<script type="text/javascript" language="Javascript">

    // Access the ViewData
    //var AuthToken = ('@ViewData["AuthToken"]');
    //var ContainerUri = ('@ViewData["ContainerUri"]');
    //var ContainerSasToken = ('@ViewData["ContainerSasToken"]');
    var OrganizationId = ('@ViewData["OrganizationId"]');
    var ObjectIdentifier = ('@ViewData["ObjectIdentifier"]');
    var SurName = ('@ViewData["SurName"]');
    var GivenName = ('@ViewData["GivenName"]');
    var ImgUrl = ('@ViewData["ImgUrl"]');
    var LogoUrl =('@ViewData["LogoUrl"]');
    var LastLogin = ('@ViewData["LastLogin"]');
    var UserId = ('@ViewData["UserId"]');
    // store the ViewData in session storage
    //sessionStorage.setItem("AuthToken", AuthToken);
    //sessionStorage.setItem("ContainerUri", ContainerUri);
    //sessionStorage.setItem("ContainerSasToken", ContainerSasToken);
    localStorage.setItem("OrganizationId", OrganizationId);
    localStorage.setItem("ObjectIdentifier", ObjectIdentifier);
    localStorage.setItem("SurName", SurName);
    localStorage.setItem("GivenName", GivenName);
    localStorage.setItem("ImgUrl", ImgUrl);
    localStorage.setItem("LogoUrl", LogoUrl);
    localStorage.setItem("LastLogin", LastLogin);
    localStorage.setItem("UserId", UserId);
</script>
<app-root>
    <div class="splash-screen">
        <div class="splash-wrapper">
            <div class="splash-logo"><img src="~/assets/layout/images/schoolwayz.png"></div>
            <div class="splash-loading"><img src="assets/layout/images/dots.svg"></div>
        </div>
    </div>

</app-root>
